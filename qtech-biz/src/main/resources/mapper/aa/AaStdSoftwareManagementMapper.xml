<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qtech.aa.mapper.AaStdSoftwareManagementMapper">
    <resultMap id="BaseResultMap" type="com.qtech.aa.domain.AaSoftwareManagement">
        <id column="id" property="id" jdbcType="BIGINT"/>
        <result column="prod_type" property="prodType" jdbcType="VARCHAR"/>
        <result column="file_name" property="fileName" jdbcType="VARCHAR"/>
        <result column="version" property="version" jdbcType="VARCHAR"/>
        <result column="upload_date" property="uploadDate" jdbcType="TIMESTAMP"/>
        <result column="path" property="path" jdbcType="VARCHAR"/>
        <result column="size" property="size" jdbcType="VARCHAR"/>
        <result column="status" property="status" jdbcType="INTEGER"/>
        <result column="author" property="author" jdbcType="VARCHAR"/>
        <result column="factory_name" property="factoryName" jdbcType="VARCHAR"/>
        <result column="remark" property="remark" jdbcType="VARCHAR"/>
    </resultMap>

    <select id="selectAaSoftwareManagementList" resultType="com.qtech.aa.domain.AaSoftwareManagement">
        SELECT id,
        prod_type,
        file_name,
        version,
        upload_date,
        `path`,
        `size`,
        status,
        author,
        factory_name,
        remark
        FROM qtech_im_chk.aa_software_file_manager
        <where>
            <if test="id != null">
                id = #{id}
            </if>
            <if test="prodType != null and prodType != ''">
                AND prod_type = #{prodType}
            </if>
        </where>
    </select>
</mapper>